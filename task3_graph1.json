{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
       "width": 600,
        "height": 400,
        "title": {
          "text": "Child Poverty Rates",
          "subtitle": ["Poverty Rates Aggregated from Local Authority level to Region level", ""],
          "fontSize": 20,
          "color": "#900C3F",
          "subtitleFontSize": 16,
          "subtitleFontWeight": "lighter",
          "subtitleColor": "#C70039",
          "anchor": "middle"
        },

    "data": {"url": "child_poverty_region.csv"},
    "mark": {
      "type": "line",
      "point": true
    },
    "encoding": {
      "x": {"field": "Year"},
      "y": {"field": "Percentage", "type": "quantitative", "grid": false},
      "color": {"field": "Region", "type": "nominal"}
    }
  }


   
